{
  "name": "typescript-course",
  "version": "1.0.0",
  "description": "API usando node,typescrip,sequelize e postgree",
  "main": "server/server.ts",
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/ts-node ./server/server.ts",
    "builded": " NODE_ENV=production node ./dist/server.js",
    "unit-test": "NODE_ENV=test mocha --opts ./dist/server/config/test/mocha.opts ./dist/modules/**/unit.test.js",
    "integration-test": "NODE_ENV=test mocha --opts ./dist/server/config/test/mocha.opts ./dist/modules/**/integration.test.js",
    "unit-local-test": "NODE_ENV=testlocal mocha --opts ./dist/server/config/test/mocha.opts ./dist/modules/**/unit.test.js",
    "integration-local-test": "NODE_ENV=testlocal mocha --opts ./dist/server/config/test/mocha.opts ./dist/modules/**/integration.test.js",
    "integration-coverage": "NODE_ENV=testlocal nyc --reporter=html  mocha --opts ./dist/server/config/test/mocha.opts ./dist/modules/**/integration.test.js",
    "unit-coverage": "NODE_ENV=testlocal nyc --reporter=html mocha --opts ./dist/server/config/test/mocha.opts ./dist/modules/**/unit.test.js"
  },
  "keywords": [
    "API",
    "TypeScript",
    "Node",
    "ES6"
  ],
  "author": "João Graça",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "bluebird": "^3.5.4",
    "body-parse": "^0.1.0",
    "express": "^4.17.0",
    "http": "0.0.0",
    "http-status": "^1.3.2",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.11.0",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.26",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/jwt-simple": "^0.5.33",
    "@types/lodash": "^4.14.129",
    "@types/mocha": "^5.2.6",
    "@types/morgan": "^1.7.35",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/sequelize": "^4.28.1",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-typescript": "^5.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "sequelize": "^5.8.6",
    "supertest": "^4.0.2",
    "testdouble": "^3.11.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.4.5"
  }
